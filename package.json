{
  "name": "freelance-bidding-platform",
  "version": "1.0.0",
  "description": "A freelance bidding platform like Upwork",
  "scripts": {
    "start": "cd server && node server.js",
    "dev": "cd server && nodemon server.js",
    "client": "cd frontend && npm run dev",
    "client:build": "cd frontend && npm run build",
    "client:install": "cd frontend && npm install",
    "server:install": "cd server && npm install",
    "install:all": "npm run server:install && npm run client:install",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["freelance", "bidding", "platform", "upwork"],
  "author": "",
  "license": "MIT"
}
